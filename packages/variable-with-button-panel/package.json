{
	"name": "@skriptfabrik/variable-with-button-panel",
	"type": "module",
	"version": "1.0.0",
	"description": "Extends the global variable panel with a button to save the variable.",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-panel"
	],
	"icon": "extension",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "panel",
		"path": "dist/index.js",
		"source": "src/index.ts",
		"host": "^10.10.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"validate": "directus-extension validate"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "17.0.5",
		"@directus/types": "^14.0.0",
		"typescript": "^5.9.3",
		"vue": "^3.5.27"
	}
}
